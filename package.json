{
  "name": "react-form-essentials",
  "version": "0.0.7",
  "description": "React form components with validations",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bionaut/react-form-essentials/"
  },
  "scripts": {
    "prepublish": "rm -rf dist && mkdir dist && cp -r src/* dist && ./node_modules/stylus/bin/stylus -c src/index.styl --out dist/styles.css",
    "start": "webpack-dev-server",
    "test": "mocha .scripts/mocha_boot.js src/**/tests/**/*.js --require babelhook --require css-null-compiler",
    "test-watch": "npm run test -- --watch --watch-extensions js"
  },
  "files":[
    "dist"
  ],
  "keywords": [
    "react"
  ],
  "author": "Janci <janci.krajnak@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.2",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^15.0.2",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.0.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "mocha-stylus-compiler": "^1.0.1"
  },
  "dependencies": {
    "material-ui": "^0.15.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-tap-event-plugin": "^1.0.0",
    "validator": "^5.2.0"
  }
}
